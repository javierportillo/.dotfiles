{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/master/schema.json",
  "animation": {
    "enabled": true,
    "duration": 300,
    "fps": 120,
    "style": "EaseInOutSine"
  },
  "app_specific_configuration_path": "$Env:USERPROFILE/applications.yaml",
  "border": true,
  "theme": {
    "palette": "Base16",
    "name": "TokyoNightTerminalStorm"
  },
  "border_implementation": "Komorebi",
  "border_style": "Square",
  "border_width": 6,
  "border_offset": -1,
  "default_workspace_padding": 4,
  "default_container_padding": 4,
  "display_index_preferences": {
    "0": "DELA0EC-5&33963ada&0&UID176385",
    "1": "DELA199-5&33963ada&0&UID176387"
  },
  "monitors": [
    {
      "window_based_work_area_offset_limit": 1,
      "window_based_work_area_offset": {
        "top": 0,
        "bottom": 0,
        "left": 300,
        "right": 600
      },
      "workspaces": [
        {
          "name": "Primary",
          "layout_rules": {
            "1": "BSP",
            "5": "Grid"
          },
          "initial_workspace_rules": [
            {
              "id": "wezterm-gui.exe",
              "kind": "Exe",
              "matching_strategy": "Equals"
            },
            {
              "id": "WindowsTerminal.exe",
              "kind": "Exe",
              "matching_strategy": "Equals"
            }
          ]
        },
        {
          "name": "Other",
          "layout_rules": {
            "1": "BSP",
            "5": "Grid"
          },
          "initial_workspace_rules": [
            {
              "id": "Netflix",
              "kind": "Title",
              "matching_strategy": "Equals"
            },
            {
              "id": "Spotify.exe",
              "kind": "Exe",
              "matching_strategy": "Equals"
            }
          ]
        }
      ]
    },
    {
      "workspaces": [
        {
          "name": "Secondary",
          "layout_rules": {
            "1": "BSP",
            "5": "Grid"
          },
          "initial_workspace_rules": [
            {
              "id": "zen.exe",
              "kind": "Exe",
              "matching_strategy": "Equals"
            },
            {
              "id": "Obsidian.exe",
              "kind": "Exe",
              "matching_strategy": "Equals"
            },
            {
              "id": "explorer.exe",
              "kind": "Exe",
              "matching_strategy": "Equals"
            }
          ]
        },
        {
          "name": "Chat",
          "layout_rules": {
            "1": "BSP",
            "5": "Grid"
          },
          "initial_workspace_rules": [
            {
              "id": "WhatsApp",
              "kind": "Title",
              "matching_strategy": "Equals"
            },
            {
              "id": "Discord.exe",
              "kind": "Exe",
              "matching_strategy": "Equals"
            },
            {
              "id": "Messenger.exe",
              "kind": "Exe",
              "matching_strategy": "Equals"
            }
          ]
        }
      ]
    }
  ],
  "stackbar": {
    "height": 20,
    "label": "Title",
    "mode": "OnStack",
    "tabs": {
      "focused_text": "#50F020",
      "unfocused_text": "#b3b3b3",
      "background": "#202020",
      "font_family": "JetBrains Mono",
      "font_size": 16
    }
  },
  "transparency": false,
  "transparency_alpha": 245,
  "window_hiding_behaviour": "Cloak",
  "float_rules": [
    {
      "kind": "Title",
      "id": "komorebi-bar",
      "matching_strategy": "Equals"
    }
  ]
}
